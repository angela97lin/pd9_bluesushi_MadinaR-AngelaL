    public CardStack(){
	stack = new Stack<Card>();
    }

    public Card pop(){
	return stack.pop();
    }

    public int peek(){
	return stack.peek().getIntVal();
    }

    public void add(Card c){
	stack.push(c);
	c.setXCor(GamePlay.myWidth/2-37);
	c.setYCor(GamePlay.myHeight/2-60);
    }


}
